		///queue
		Queue<String> obj2=new LinkedList<String>();
		obj2.add("Love");
		obj2.add("jealas");
		obj2.add("hate");
		obj2.add("like");
		obj2.add("Pleasant");
		obj2.add("EyeCandy");
		
		System.out.println(obj2.peek());
		Iterator it=obj2.iterator();
		while(it.hasNext()==true){
			System.out.println(it.next());
			obj2.poll();
			if (obj2.poll()==null){
			break;	
			}
		}
		
	}

}
